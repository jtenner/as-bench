  },
  "homepage": "https://github.com/jtenner/as-bench#readme",
  "dependencies": {
    "@as-bench/assembly": "^0.0.1",
    "@as-bench/core": "^0.0.1",
    "ansi-colors": "^4.1.1",
    "node-glob": "^1.2.0"
  },
  "peerDependencies": {
    "assemblyscript": "^0.18.12"
  },
